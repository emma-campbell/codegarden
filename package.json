{
  "name": "codegarden",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "yarn copy-shiki && next dev",
    "build": "yarn copy-shiki && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "staged": "lint-staged",
    "prepare": "husky install",
    "format": "prettier --write",
    "copy-shiki": "cp node_modules/shiki/languages/* shiki/languages && cp node_modules/shiki/themes/* shiki/themes"
  },
  "dependencies": {
    "@fontsource/fira-code": "^4.5.13",
    "@fontsource/inter": "^5.0.1",
    "@heroicons/react": "^2.0.14",
    "@planetscale/database": "^1.7.0",
    "@vercel/analytics": "^1.0.1",
    "cal-sans": "^1.0.1",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "contentlayer": "^0.3.1",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.11.2",
    "github-slugger": "^2.0.0",
    "kysely": "^0.24.2",
    "kysely-planetscale": "^1.3.0",
    "moment": "^2.29.4",
    "next": "^13.4.3",
    "next-contentlayer": "^0.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.9.4",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "server-only": "^0.0.1",
    "shiki": "^0.14.1",
    "swr": "^2.1.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.14.6",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.1.0-beta"
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
